# DB
POSTGRES_HOST=db
POSTGRES_DB=vibescope
POSTGRES_USER=vibe
POSTGRES_PASSWORD=vibe
POSTGRES_PORT=5432

# Optional override: set a full URL (e.g., SQLite for local dev)
# DATABASE_URL=sqlite:///../../data/app.db
# AUTO_MIGRATE=1

# ListenBrainz / MusicBrainz
LISTENBRAINZ_USER=your_user
LISTENBRAINZ_TOKEN=lb_xxx
MUSICBRAINZ_RATE_LIMIT=1.0

# Last.fm
LASTFM_API_KEY=lfm_xxx
LASTFM_USER=your_lastfm_username

# Auth.js (NextAuth) â€“ Google OAuth
NEXTAUTH_URL=https://your.domain
NEXTAUTH_SECRET=supersecretlongrandom
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# UI
NEXT_PUBLIC_API_BASE=http://localhost:8000

# Paths
AUDIO_ROOT=/audio
CACHE_DIR=/tmp/vibescope
EXCERPT_SECONDS=0

# Models
EMBEDDING_MODEL=openl3,musicnn,clap,panns
USE_CLAP=true
USE_DEMUCS=false
TORCH_DEVICE=auto

# Scheduler
DEFAULT_USER_ID=your-user
INGEST_LISTENS_INTERVAL_MINUTES=1
LASTFM_SYNC_INTERVAL_MINUTES=30
AGGREGATE_WEEKS_INTERVAL_MINUTES=1440
TZ=America/New_York
